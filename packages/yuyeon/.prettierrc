{
  "singleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 80,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^%/(.*)$",
    "^@/(.*)$",
    "^[./].*(?<!\\.(?:scss|css))$",
    "^[./](.*)\\.(s?css)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
